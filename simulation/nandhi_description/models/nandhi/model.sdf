<?xml version="1.0"?>
<!--
  Gazebo Ackermann steering plugin demo

  Try sending commands:

    gz topic -t "/model/nandhi/cmd_vel" -m gz.msgs.Twist -p "linear: {x: 0.5}, angular: {z: 0.1}"

  Listen to odometry:

    gz topic -e -t /model/nandhi/odometry

-->
<sdf version="1.8">
  <model name='nandhi'>

    <link name='chassis'>
      <pose>-0.01 -0 0.025 0 0 0</pose>
      <inertial>
        <mass>3</mass>
        <inertia>
          <ixx>0.126164</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.416519</iyy>
          <iyz>0</iyz>
          <izz>0.481014</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <box>
            <size>0.532 0.29 0.195</size>
          </box>
        </geometry>
        <material>
          <ambient>0.5 0.5 1.0 1</ambient>
          <diffuse>0.5 0.5 1.0 1</diffuse>
          <specular>0.0 0.0 1.0 1</specular>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <box>
            <size>0.532 0.29 0.195</size>
          </box>
        </geometry>
